{
  "name": "ffmpeg-server",
  "version": "0.1.0",
  "description": "Express server with TypeScript for ffmpeg processing",
  "main": "dist/index.js",
  "type": "module",
  "packageManager": "pnpm@10.1.0",
  "engines": {
    "node": ">=24.11.1",
    "pnpm": "10.1.0"
  },
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "pnpm lint && rm -rf dist && tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "keywords": [
    "express",
    "typescript",
    "ffmpeg",
    "server"
  ],
  "author": "Udayan Maurya",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}
